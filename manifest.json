{
  "manifest_version": 3,
  "name": "Simple Note",
  "version": "1.4.1",
  "description": "A simple note Chrome extension",
  "permissions": ["storage", "downloads", "clipboardRead"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}